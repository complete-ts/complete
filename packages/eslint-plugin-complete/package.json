{
  "name": "eslint-plugin-complete",
  "version": "1.1.3",
  "description": "An ESLint plugin that contains useful rules.",
  "keywords": [
    "eslint",
    "plugin",
    "complete",
    "lint",
    "linting",
    "rules",
    "enum",
    "enums",
    "comment",
    "comments",
    "format",
    "autoformat",
    "prettier",
    "sentences"
  ],
  "homepage": "https://complete-ts.github.io/",
  "bugs": {
    "url": "https://github.com/complete-ts/complete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complete-ts/complete.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "module",
  "main": "./dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "create-rule": "tsx ./scripts/createRule.ts",
    "docs": "eslint-doc-generator",
    "generate": "tsx ./scripts/generate.ts",
    "lint": "tsx ./scripts/lint.ts",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/type-utils": "8.46.0",
    "@typescript-eslint/utils": "8.46.0",
    "typescript-eslint": "8.46.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.2",
    "@typescript-eslint/rule-tester": "8.46.0",
    "@typescript-eslint/types": "8.46.0",
    "complete-common": "2.8.0",
    "complete-node": "12.2.1",
    "eslint-doc-generator": "2.3.0",
    "jest": "30.2.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0",
    "typescript": ">= 5.0.0"
  }
}
