{
  "name": "eslint-plugin-complete",
  "version": "1.2.4",
  "description": "An ESLint plugin that contains useful rules.",
  "keywords": [
    "eslint",
    "plugin",
    "complete",
    "lint",
    "linting",
    "rules",
    "enum",
    "enums",
    "comment",
    "comments",
    "format",
    "autoformat",
    "prettier",
    "sentences"
  ],
  "homepage": "https://complete-ts.github.io/",
  "bugs": {
    "url": "https://github.com/complete-ts/complete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complete-ts/complete.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "module",
  "main": "./dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run ./scripts/build.ts",
    "create-rule": "bun run ./scripts/createRule.ts",
    "docs": "bun run build && eslint-doc-generator",
    "generate": "bun run ./scripts/generate.ts",
    "lint": "bun run ./scripts/lint.ts",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/type-utils": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@babel/preset-env": "catalog:",
    "@babel/preset-typescript": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/rule-tester": "catalog:",
    "@typescript-eslint/types": "catalog:",
    "complete-common": "workspace:*",
    "complete-node": "workspace:*",
    "eslint-doc-generator": "catalog:",
    "jest": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0",
    "typescript": ">= 5.0.0"
  }
}
