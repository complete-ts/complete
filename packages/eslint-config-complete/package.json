{
  "name": "eslint-config-complete",
  "version": "4.2.0",
  "description": "A sharable ESLint config for TypeScript projects.",
  "keywords": [
    "eslint",
    "config",
    "configs",
    "lint",
    "linting",
    "prettier"
  ],
  "homepage": "https://complete-ts.github.io/",
  "bugs": {
    "url": "https://github.com/complete-ts/complete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complete-ts/complete.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run ./scripts/build.ts",
    "docs": "bun run ./scripts/docs.ts",
    "lint": "bun run ./scripts/lint.ts"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "catalog:",
    "confusing-browser-globals": "catalog:",
    "eslint-import-resolver-typescript": "catalog:",
    "eslint-plugin-complete": "workspace:*",
    "eslint-plugin-eslint-plugin": "catalog:",
    "eslint-plugin-import-x": "catalog:",
    "eslint-plugin-jsdoc": "catalog:",
    "eslint-plugin-n": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "jsonc-eslint-parser": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/confusing-browser-globals": "catalog:",
    "@types/eslint-config-prettier": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "complete-common": "workspace:*",
    "complete-node": "workspace:*",
    "eslint-config-prettier": "catalog:",
    "extract-comments": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0"
  }
}
