{
  "name": "eslint-config-complete",
  "version": "4.1.4",
  "description": "A sharable ESLint config for TypeScript projects.",
  "keywords": [
    "eslint",
    "config",
    "configs",
    "lint",
    "linting",
    "prettier"
  ],
  "homepage": "https://complete-ts.github.io/",
  "bugs": {
    "url": "https://github.com/complete-ts/complete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complete-ts/complete.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run ./scripts/build.ts",
    "docs": "bun run ./scripts/docs.ts",
    "lint": "bun run ./scripts/lint.ts"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "5.7.0",
    "confusing-browser-globals": "1.0.11",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-complete": "workspace:*",
    "eslint-plugin-eslint-plugin": "7.3.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "62.0.0",
    "eslint-plugin-n": "17.23.2",
    "eslint-plugin-unicorn": "62.0.0",
    "jsonc-eslint-parser": "2.4.2",
    "typescript-eslint": "8.53.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/confusing-browser-globals": "1.0.3",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "25.0.9",
    "complete-common": "workspace:*",
    "complete-node": "workspace:*",
    "eslint-config-prettier": "10.1.8",
    "extract-comments": "1.1.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0"
  }
}
