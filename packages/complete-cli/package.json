{
  "name": "complete-cli",
  "version": "1.0.1-dev.6",
  "description": "A command line tool for bootstrapping TypeScript projects.",
  "keywords": [
    "typescript"
  ],
  "homepage": "https://complete-ts.github.io/",
  "bugs": {
    "url": "https://github.com/complete-ts/complete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complete-ts/complete.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "module",
  "bin": {
    "complete-cli": "./dist/main.js"
  },
  "files": [
    "dist",
    "file-templates",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "lint": "tsx ./scripts/lint.ts",
    "start": "tsx ./src/main.ts",
    "test": "glob \"./src/**/*.test.ts\" --cmd=\"node --import tsx --test --test-reporter spec\""
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "chalk": "^5.4.1",
    "clipanion": "^4.0.0-rc.4",
    "complete-common": "^1.0.1",
    "complete-node": "^1.7.4",
    "execa": "^9.5.2",
    "klaw-sync": "^6.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/klaw-sync": "^6.0.5",
    "@types/node": "^22.13.4",
    "@types/prompt": "^1.1.9",
    "complete-node": "^1.7.4",
    "eslint-plugin-sort-exports": "^0.9.1",
    "glob": "^11.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "unbuild": "^3.3.1"
  },
  "engines": {
    "node": ">= 20.11.0"
  }
}
