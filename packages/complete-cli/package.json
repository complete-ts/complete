{
  "name": "complete-cli",
  "version": "1.2.0",
  "description": "A command line tool for bootstrapping TypeScript projects.",
  "keywords": [
    "typescript"
  ],
  "homepage": "https://complete-ts.github.io/",
  "bugs": {
    "url": "https://github.com/complete-ts/complete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complete-ts/complete.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "module",
  "bin": {
    "complete-cli": "./dist/main.js"
  },
  "files": [
    "dist",
    "file-templates",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "lint": "tsx ./scripts/lint.ts",
    "start": "tsx ./src/main.ts",
    "test": "glob \"./src/**/*.test.ts\" --cmd=\"node --import tsx --test --test-reporter spec\""
  },
  "dependencies": {
    "@zamiell/clack-prompts": "0.10.2",
    "chalk": "5.4.1",
    "clipanion": "4.0.0-rc.4",
    "complete-common": "2.2.0",
    "complete-node": "5.1.3",
    "klaw-sync": "7.0.0",
    "yaml": "2.7.1"
  },
  "devDependencies": {
    "@types/klaw-sync": "6.0.5",
    "@types/node": "22.14.1",
    "glob": "11.0.1",
    "ts-loader": "9.5.2",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "webpack": "5.99.6",
    "webpack-cli": "6.0.1",
    "webpack-shebang-plugin": "1.1.8"
  },
  "engines": {
    "node": ">= 20.11.0"
  }
}
